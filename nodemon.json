{
    "restartable": "rs",
    "verbose": true,
    "ignore": [
        "build/**",
        "client/**",
        "node_modules/**/node_modules",
        "notes/*",
        ".babelrc",
        ".editconfig",
        ".eslintrc",
        ".gitignore",
        ".git/**",
        ".package.json",
        "yarn.lock"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "execMap": {
        "rb": "ruby",
        "pde": "processing --sketch={{pwd}} --run"
    },
    "ext": "js"
}